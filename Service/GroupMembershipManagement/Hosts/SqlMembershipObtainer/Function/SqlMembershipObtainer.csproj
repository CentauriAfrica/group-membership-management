    <ProjectReference Include="..\..\SqlMembershipObtainer.Common\DependencyInjection\SqlMembershipObtainer.Common.DependencyInjection.csproj" />
    <ProjectReference Include="..\..\SqlMembershipObtainer.Common\Repositories.DataFactory\Repositories.DataFactory.csproj" />
