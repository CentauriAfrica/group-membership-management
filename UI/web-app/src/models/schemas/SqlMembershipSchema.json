      "type": "string",
      "enum": ["SqlMembership"]
    },
    "source": {
      "type": "object",
      "properties": {
        "ids": {
          "type": "array",
          "minItems": 1,
          "uniqueItems": true,
          "items": {
            "type": "integer"
          }
        },
        "filter": {
          "type": "string",
          "minLength": 1
        },
        "depth": {
          "type": "integer",
          "minimum": 1
        }
      },
      "additionalProperties": false
    },
    "exclusionary": {
      "type": "boolean"
    }
  },
  "required": ["type", "source"],
  "additionalProperties": false
}
  